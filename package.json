{
  "name": "improv",
  "version": "0.5.1",
  "description": "A model-backed generative text grammar tool for javascript",
  "homepage": "",
  "author": {
    "name": "Bruno Dias",
    "email": "bruno.r.dias@gmail.com",
    "url": "http://segue.pw/"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "keywords": [
    ""
  ],
  "devDependencies": {
    "babel-core": "^5.5.0",
    "babel-eslint": "^4.1.6",
    "del": "^2.0.2",
    "eslint-config-xo-space": "^0.7.0",
    "eslint-plugin-babel": "^3.0.0",
    "fs-jetpack": "^0.7.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-eslint": "^1.0.0",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.0.0",
    "gulp-nsp": "^2.1.0",
    "gulp-plumber": "^1.0.0",
    "isparta": "^3.0.3",
    "lodash": "^4.0.0",
    "should": "^8.2.0",
    "simple-mock": "^0.5.0"
  },
  "eslintConfig": {
    "extends": "xo-space/esnext",
    "env": {
      "mocha": true
    },
    "rules": {
      "prefer-arrow-callback": 0,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "curly": [
        2,
        "multi-line"
      ],
      "padded-blocks": 0,
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "babel/object-curly-spacing": [
        2,
        "always"
      ],
      "new-cap": [
        2,
        {
          "capIsNew": false,
          "newIsCap": true
        }
      ],
      "prefer-reflect": [
        2,
        {
          "exceptions": [
            "apply",
            "call"
          ]
        }
      ]
    }
  },
  "repository": "sequitur/improv",
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.0.1"
  }
}
